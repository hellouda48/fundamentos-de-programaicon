Algoritmo sin_titulo
	Escribir "Teclea cuantas tarjetas tienes(maximo 3)"
	Leer n
	Segun n Hacer
		1:
			Escribir "Que tipo de tarjeta tienes"
			leer n1
		2:
			Escribir "Que tipo de tarjetas tienes"
			Escribir "Tarjeta 1"
			Leer n1
			Escribir "Tarjeta 2"
			Leer n2
			Escribir "Quieres elegir tu propia tarjeta"
			Leer resp
			Si resp="si" Entonces
				Escribir "cual tarjeta quieres usar?"
				Leer resp2
				Segun resp2 Hacer
					1:
						tarj=n1
					2:
						tarj=n2
					
				Fin Segun
			SiNo
				Si n1<n2 Entonces
					tarj=n2
				SiNo
					tarj=2
				Fin Si
			Fin Si
			
		3:
			Escribir "Que tipo de tarjetas tienes"
			Escribir "Tarjeta 1"
			Leer n1
			Escribir "Tarjeta 2"
			Leer n2
			Escribir "Tarjeta 3"
			Leer n3
			Escribir "Quieres elegir tu propia tarjeta"
			Leer resp
			Si resp="si" Entonces
				Escribir "cual tarjeta quieres usar?"
				Leer resp2
				Segun resp2 Hacer
					1:
						tarj=n1
					2:
						tarj=n2
					3: tarj=n3
						
				Fin Segun
			SiNo
				Si n1<n2 Entonces
					Si n2<n3 Entonces
						tarj=n3
					SiNo
						tarj=n2
					Fin Si
				SiNo
					Si n1<n3 Entonces
						tarj=n1
					SiNo
						tarj=n3
					Fin Si
				Fin Si
			Fin Si
			
			
		De Otro Modo:
			Escribir "Ingresaste una cantidad no valida de tarjetas"
	Fin Segun
	
	Segun tarj Hacer
		1:
			Escribir "El aumento de credito es de 25%"
		2:
			Escribir "El aumento de credito es de 35%"
		3:
			Escribir "El aumento de credito es de 40%"
		De Otro Modo:
			Escribir "El aumento de credito es del 50%"
	Fin Segun
FinAlgoritmo
